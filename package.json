{
  "name": "dotcode-gui",
  "version": "1.0.0",
  "main": "main.js",
  "description": "DotCode generator with text replacement",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "bwip-js": "^4.6.0"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.dotcode.gui",
    "productName": "DotCodeGUI",
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "renderer.js",
      "node_modules/**"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": ["x64"]
        }
      ],
      "artifactName": "DotCodeGUI.exe"
    },
    "compression": "maximum"
  }
}